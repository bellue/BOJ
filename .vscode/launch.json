{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C++ Debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}\\${fileBasenameNoExtension}.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": true,
      "setupCommands": [
        {
          "description": "Enable pretty-printing for array",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set print array elements limit",
          "text": "set print elements 0",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
